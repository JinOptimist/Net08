@model CatViewModel

@section Head{
    <script src="~/js/catAdd.js"></script>
}

<form action="/Cat/Add" method="post" id="someForm">
    <div>
        Name: <input name="name" />
        <img class="icon-add-cat" id="good" src="~/image/GreenYes.png" />
        <img class="icon-add-cat" id="bad" src="~/image/RedNo.png" />
    </div>
    <div>
        Second: <input name="second" type="number" />
    </div>
    <div>
        url: <input name="url" />
    </div>
    <div>
        <input type="submit" value="Send" />
    </div>
</form>

